import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int n1 = sc.nextInt();
        int sum = 0;
        int product = 1;
        while(n1>0){
            sum=sum+(n1%10);
            product = product *(n1%10);
            n1=n1 / 10;   
        }
        System.out.println(product-sum);
    }
}
